void bGrad() : 
{}
{
  //((<EINS> | <ZAHLEN2_7> | <ACHT>) nullBisNeun() | nullBisNeun() | <NEUN> <NULL>) "\u00b0" 
  <B_GRADZAHL> <GRAD_MIN> <GRAD_SEK> "#" (<N> | <S>)
}

void lGrad() :  	
{}
{
  //([<EINS>] [<ZAHLEN2_7>] nullBisNeun() | <EINS> <ACHT> <NULL> | (<ACHT> | <NEUN>) nullBisNeun()) "\u00b0"
  ((<L_GRADZAHL> | <B_GRADZAHL> ) <GRAD_MIN> <GRAD_SEK> "#") (<O> | <W>)
}

