void sos() :
{}
{
  <S> ( sos() | <O> ) <S> 
}
 

void worte() :
{}
{
  ("#" | wort() "#") [worte()]
}

void wort() :
{}
{    morseZeichen() [wort()]
}


void hinten() :
{}
{
  morseZeichen() ["#"] [hinten()]
}



